<h1>Listing timed_assets</h1>

<p style="padding-left:15px"> 
Items count: <%= TimedAsset.count %> <br>
Time Logged: <%= TimedAsset.count %> minutes <br>
Time Logged: <%= TimedAsset.count  / 60 %> hours <br>
Time Logged: <%= TimedAsset.count  / 60 / 24 %> days
</p>

<table style="margin-left:15px">
  <thead>
    <tr>
      <th>Btc</th>
      <th>Ltc</th>
      <th>Ppc</th>
      <th>Nmc</th>
      <th>Xpm</th>
      <th>Asicminer</th>
      <th>Asicminer small</th>
      <th>Advanced mining corp</th>
      <th>Misc1</th>
      <th>Misc2</th>
      <th>Misc3</th>
      <th>Comment</th>
      <th>Time changed</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @timed_assets.each do |timed_asset| %>
      <tr>
        <td><%= timed_asset.BTC %></td>
        <td><%= timed_asset.LTC %></td>
        <td><%= timed_asset.PPC %></td>
        <td><%= timed_asset.NMC %></td>
        <td><%= timed_asset.XPM %></td>
        <td><%= timed_asset.AsicMiner %></td>
        <td><%= timed_asset.AsicMiner_small %></td>
        <td><%= timed_asset.Advanced_Mining_Corp %></td>
        <td><%= timed_asset.misc1 %></td>
        <td><%= timed_asset.misc2 %></td>
        <td><%= timed_asset.misc3 %></td>
        <td><%= timed_asset.comment %></td>
        <td><%= timed_asset.time_changed %></td>
        <td><%= link_to 'Show', timed_asset %></td>
        <td><%= link_to 'Edit', edit_timed_asset_path(timed_asset) %></td>
        <td><%= link_to 'Destroy', timed_asset, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Timed asset', new_timed_asset_path %>
